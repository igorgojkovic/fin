USE OT0S IN 0
USE R1 IN 0
SELECT R1
GO TOP
DO WHILE.NOT.EOF()
   MSASIJA=SASIJA
   MGOD3=GOD3
   MDAT3=DAT3
   MDDAT=DDAT
   SCATTER NAME POLJA
   SELECT OT0S
   LOCATE FOR SASIJA=MSASIJA
   IF FOUND()
      REPLACE GOD3 WITH MGOD3
      REPLACE DAT3 WITH MDAT3
   ELSE
      APPEND BLANK
      GATHER NAME POLJA   
   ENDIF 
   SELECT R1
   SKIP
ENDDO   
