mpib=blptsupl.txtpib.value
IF mpib=SPACE(13)
SELECT BLP
SELECT TS
   SET ORDER TO 1
   DO FORM BLPTSA
   SELECT TS
   MPIB=PIB
   MIME=IME
   MPREZ=PREZ
   MADRESA=ULICA
   MOPIS='KOMUNALNE USLUGE '
