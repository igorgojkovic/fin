   SELECT KALPRN
   LOCATE FOR FVRSTA=MFVRSTA
   IF FOUND()
      MFNAZ=FNAZ
   ELSE
      MFNAZ='*****NEMA****'   
   ENDIF
   SELECT KOCKA
   REPLACE NAZIV WITH MFNAZ   
   SKIP
ENDDO   
GO TOP
